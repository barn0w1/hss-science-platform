{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "check-types": {},
    "db:push": {
      "cache": false,
      "interactive": true
    },
    "db:generate": {
      "cache": false
    }
  }
}